@model int[]

<div id="chart"></div>

<script src="https://cdn.jsdelivr.net/npm/frappe-charts@1.1.0/dist/frappe-charts.min.iife.js"></script>

<script>
    data = {
        labels: ["Sun", "Mon", "Tue", "Wed", "Thu", "Fri", "Sat", "Sun"],
        datasets: [
            { values: [18, 40, 30, 35, 8, 52, 17, 4] }
        ]
    }
    new frappe.Chart("#chart", {
        data: data,
        type: 'bar',
        height: 140,
        colors: ['red']
    });
</script>